<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<welcome-file-list>
		<welcome-file>/index/index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>startServlet</servlet-name>
		<servlet-class>start.StartContext</servlet-class>
		<load-on-startup></load-on-startup>
	</servlet>



	<servlet>
		<servlet-name>index</servlet-name>
		<jsp-file>/index/index.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/login/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>apply</servlet-name>
		<jsp-file>/apply/apply.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>apply</servlet-name>
		<url-pattern>/apply</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>review</servlet-name>
		<jsp-file>/review/review.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>review</servlet-name>
		<url-pattern>/review</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>system</servlet-name>
		<jsp-file>/system/system.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>system</servlet-name>
		<url-pattern>/system</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>subject</servlet-name>
		<jsp-file>/subject/subject.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>subject</servlet-name>
		<url-pattern>/subject</url-pattern>
	</servlet-mapping>

<servlet>
		<servlet-name>message</servlet-name>
		<jsp-file>/message/message.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>message</servlet-name>
		<url-pattern>/message</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>file</servlet-name>
		<servlet-class>source.FileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>file</servlet-name>
		<url-pattern>/file</url-pattern>
	</servlet-mapping>


</web-app>
